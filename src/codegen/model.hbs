{{#each imports}}
import { {{name}} } from '{{path}}';
{{/each}}
export class {{name}} {

{{#each properties}}
    public {{name}}!: {{model.name}};
{{/each}}

    public constructor(raw: {{name}}) {
{{#each properties}}
    {{#if isClassModel}}
        this.{{name}} = new {{model.name}}(raw.{{name}});
    {{else}}
        this.{{name}} = raw.{{name}};
    {{/if}}
{{/each}}
    }
}