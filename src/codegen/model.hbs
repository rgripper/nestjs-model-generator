{{#each imports}}
import { {{name}} } from '{{modulePath}}';
{{/each}}
export class {{model.name}} {

{{#each model.properties}}
    public {{name}}!: {{model.name}};
{{/each}}

    public constructor(raw: {{model.name}}) {
{{#each model.properties}}
    {{#if isCustom}}
        this.{{name}} = new {{model.name}}(raw.{{name}});
    {{else}}
        this.{{name}} = raw.{{name}};
    {{/if}}
{{/each}}
    }
}